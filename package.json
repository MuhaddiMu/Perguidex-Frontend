{
  "name": "perguidex",
  "version": "1.0.0",
  "description": "Perguidex Frontend & Backend View",
  "author": "Muhammad Muhaddis",
  "private": true,
  "scripts": {
    "dev": "nuxt --config-file nuxt.config.js -p=3000 --hostname 0.0.0.0",
    "build": "nuxt build --config-file nuxt.config.js",
    "start": "nuxt start --config-file nuxt.config.js",
    "generate": "nuxt generate --config-file nuxt.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/pwa": "^3.3.5",
    "aos": "^2.3.4",
    "graphql-tag": "^2.12.6",
    "moment": "^2.29.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^2.6.0",
    "jest": "^26.6.3",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.1"
  }
}
