{
  "name": "perguidex",
  "version": "1.0.0",
  "description": "Perguidex Frontend & Backend View",
  "author": "Muhammad Muhaddis",
  "private": true,
  "scripts": {
    "Main:dev": "nuxt --config-file Perguidex/nuxt.config.js -p=3000 --hostname 0.0.0.0",
    "App:dev": "nuxt --config-file Perguidex-App/nuxt.config.js -p=4000 --hostname 0.0.0.0",
    "dev": "concurrently \"npm run Main:dev\" \"npm run App:dev\"",
    "Main:build": "nuxt build --config-file Perguidex/nuxt.config.js",
    "App:build": "nuxt build --config-file Perguidex-App/nuxt.config.js",
    "Main:start": "nuxt start --config-file Perguidex/nuxt.config.js",
    "App:start": "nuxt start --config-file Perguidex-App/nuxt.config.js",
    "Main:generate": "nuxt generate --config-file Perguidex/nuxt.config.js",
    "App:generate": "nuxt generate --config-file Perguidex-App/nuxt.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "@nuxtjs/pwa": "^3.1.2",
    "aos": "^2.3.4",
    "moment": "^2.29.1",
    "nuxt": "^2.14.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-rc.0"
  }
}
